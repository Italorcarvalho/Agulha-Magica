<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletInserirUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletInserirUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLoginUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletLoginUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLogoutUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletLogoutUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAlterarUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletAlterarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAlterarSenhaUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletAlterarSenhaUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInserirEndereco</servlet-name>
        <servlet-class>br.com.bean.ServletInserirEndereco</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBuscarProdutoPorId</servlet-name>
        <servlet-class>br.com.bean.ServletBuscarProdutoPorId</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBuscarProdutoPorCategoriaId</servlet-name>
        <servlet-class>br.com.bean.ServletBuscarProdutoPorCategoriaId</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBuscarProdutoPorDescricao</servlet-name>
        <servlet-class>br.com.bean.ServletBuscarProdutoPorDescricao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInserirCarrinho</servlet-name>
        <servlet-class>br.com.bean.ServletInserirCarrinho</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletRemoverDoCarrinho</servlet-name>
        <servlet-class>br.com.bean.ServletRemoverDoCarrinho</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInserirListaDesejo</servlet-name>
        <servlet-class>br.com.bean.ServletInserirListaDesejo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletRemoverDaListaDesejo</servlet-name>
        <servlet-class>br.com.bean.ServletRemoverDaListaDesejo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMoverListaDesejoCarrinho</servlet-name>
        <servlet-class>br.com.bean.ServletMoverListaDesejoCarrinho</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletInserirUsuario</servlet-name>
        <url-pattern>/novo.usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLoginUsuario</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLogoutUsuario</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAlterarUsuario</servlet-name>
        <url-pattern>/alterar.usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAlterarSenhaUsuario</servlet-name>
        <url-pattern>/nova.senha</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInserirEndereco</servlet-name>
        <url-pattern>/novo.endereco</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletBuscarProdutoPorId</servlet-name>
        <url-pattern>/buscarprod.id</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletBuscarProdutoPorCategoriaId</servlet-name>
        <url-pattern>/buscarprod.categoriaid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletBuscarProdutoPorDescricao</servlet-name>
        <url-pattern>/buscarprod.descricao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInserirCarrinho</servlet-name>
        <url-pattern>/inserir.carrinho</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletRemoverDoCarrinho</servlet-name>
        <url-pattern>/remover.carrinho</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInserirListaDesejo</servlet-name>
        <url-pattern>/inserir.listadesejos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletRemoverDaListaDesejo</servlet-name>
        <url-pattern>/remover.listadesejos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMoverListaDesejoCarrinho</servlet-name>
        <url-pattern>/mover.listadesejos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
