<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletInserirUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletInserirUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLoginUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletLoginUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLogoutUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletLogoutUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAlterarUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletAlterarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAlterarSenhaUsuario</servlet-name>
        <servlet-class>br.com.bean.ServletAlterarSenhaUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletInserirUsuario</servlet-name>
        <url-pattern>/novo.usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLoginUsuario</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLogoutUsuario</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAlterarUsuario</servlet-name>
        <url-pattern>/alterar.usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAlterarSenhaUsuario</servlet-name>
        <url-pattern>/nova.senha</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            15
        </session-timeout>
    </session-config>
</web-app>
